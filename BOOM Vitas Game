def on_player2_connected():
    global DIMI
    DIMI = sprites.create(img("""
            ....ffffff.........ccc..
                    ....ff22ccf.......cc4f..
                    .....ffccccfff...cc44f..
                    ....cc24442222cccc442f..
                    ...c9b4422222222cc422f..
                    ..c999b2222222222222fc..
                    .c2b99111b222222222c22c.
                    c222b111992222ccccccc22f
                    f222222222222c222ccfffff
                    .f2222222222442222f.....
                    ..ff2222222cf442222f....
                    ....ffffffffff442222c...
                    .........f2cfffc2222c...
                    .........fcc2ffffffff...
                    ..........fc2ffff.......
                    ...........fffff........
        """),
        SpriteKind.player)
    DIMI.set_stay_in_screen(True)
    controller.player2.move_sprite(DIMI, 200, 200)
    info.player2.set_life(150)
    DIMI.set_position(146, 59)
    scene.camera_follow_sprite(nick)
controller.player2.on_event(ControllerEvent.CONNECTED, on_player2_connected)

def on_player1_score():
    game.game_over(True)
info.player1.on_score(200, on_player1_score)

