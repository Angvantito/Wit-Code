@namespace
class SpriteKind:
    p2 = SpriteKind.create()
    p1 = SpriteKind.create()

def on_player2_button_b_pressed():
    global projectile2
    projectile2 = sprites.create_projectile_from_sprite(img("""
            . . . . . . . . . . . . . . . . 
                    . . . . . . . . . . . . . . . . 
                    . . . . . . . . . . . . . . . . 
                    . . . . . . . . . . . . . . . . 
                    . . . . . . . . . . . . . . . . 
                    . . . . . . . . . . . . . . . . 
                    . . . . . . . . . . . . . . . . 
                    . . . . . . . . . . . . . . . . 
                    . . . . . b b b b 4 4 4 . . . . 
                    . . . . b b b b b 5 5 5 4 . . . 
                    . . . . . b b b b 4 4 4 . . . . 
                    . . . . . . . . . . . . . . . . 
                    . . . . . . . . . . . . . . . . 
                    . . . . . . . . . . . . . . . . 
                    . . . . . . . . . . . . . . . . 
                    . . . . . . . . . . . . . . . .
        """),
        DIMI,
        -300,
        0)
    music.play(music.melody_playable(music.pew_pew),
        music.PlaybackMode.UNTIL_DONE)
controller.player2.on_button_event(ControllerButton.B,
    ControllerButtonEvent.PRESSED,
    on_player2_button_b_pressed)

def on_on_overlap(sprite, otherSprite):
    sprites.destroy(sprite)
    nick.start_effect(effects.fire, 1000)
    info.player1.change_life_by(-1)
    info.player2.change_score_by(1)
    scene.camera_shake(4, 500)
sprites.on_overlap(SpriteKind.projectile, SpriteKind.food, on_on_overlap)

def on_b_pressed():
    global projectile
